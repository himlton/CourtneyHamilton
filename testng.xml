<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Courtney-Hamilton-Suite">
<listeners>
<listener class-name="com.CH.qa.util.ExtentReporterNG"></listener>
</listeners>
  <test thread-count="5" name="Courtney-Hamilton-Test" parallel="true">
  <groups>
  <run>
  <exclude name="NegativeScenario"/>
  <include name="HappyPath"/>
  </run>
  </groups>
    <classes>
      <class name="com.CH.qa.test.TellUsYourSelfTest"/>
      <class name="com.CH.qa.test.ConfirmTest"/>
      <class name="com.CH.qa.test.SuccessTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
